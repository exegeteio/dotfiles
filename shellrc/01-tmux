#!/usr/bin/env bash

[[ -z "$DEBUG" ]] || echo "Checking tmux"
if [[ -x "$(which tmux)" ]] && [[ -z "$TMUX" ]] && [[ -f "$HOME/.auto_tmux" ]] && [[ -z "$VSCODE_PID" ]] && [[ "$TERM_PROGRAM" != "vscode" ]]; then
  if [[ -z "$SSH_CLIENT" ]]; then
    tmux.re "main"
    exec tmux -t "main"
  else
    tmux.re "$(whoami)"
  fi
else
  # Can't recall why this was here?  Knock it off.
  # export TMUX=""
fi
