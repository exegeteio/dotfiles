{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#424242",
          "foreground": "#9B6BDF",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#424242",
          "foreground": "#ccc",
          "foreground_templates": [
            "{{ if .SSHSession }}#f88{{ end }}"
          ],
          "style": "diamond",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ trunc 4 .HostName | lower }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "session"
        },
        {
          "background": "#424242",
          "foreground": "#3EC669",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#424242",
          "foreground": "#43CCEA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} \u276f",
          "trailing_diamond": "\ue0b4 ",
          "type": "git"
        },
        {
          "background": "#f118fc",
          "foreground": "#242424",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "trailing_diamond": "\ue0b4 ",
          "type": "executiontime"
        },
        {
          "background": "#33DD2D",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "foreground": "#242424",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\uf7d4",
          "trailing_diamond": "\ue0b4 ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
