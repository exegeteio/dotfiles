#!/usr/bin/env bash
# Stolen from rwxrob:  github.com/rwxrob/dot

buf="$*"
if test -n "$buf"; then
  if [[ "$buf" == "wave" ]]; then
    buf="mannim4WaveAnimated"
  fi
  tmux -L stream send -t '{marked}' "$buf" Enter
  exit
fi

IFS=
while read -r line; do
  tmux -L stream send -t '{marked}' "$line" Enter
done
